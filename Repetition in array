#include<stdio.h>
void repetition(int a[],int n){
    printf("Enter the number whose repetition you want to count : ");
    int x,c=0;
    scanf("%d",&x);
    for(int i=0;i<n;i++){
        if(a[i]==x){
            c++;
        }
    }
    printf("%d Repeats in array %d times.",x,c);
    return ;
}
int main(){
    printf("Enter the number of elemnts in the array : ");
    int n;
    scanf("%d",&n);
    int array[n];
    for(int i=0;i<n;i++){
        printf("Enter the vlaue of array[%d] : ",i);
        scanf("%d",&array[i]);
    }
    repetition(array,n);
    return 0;
}
