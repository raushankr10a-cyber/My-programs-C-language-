#include<stdio.h>
#include<string.h>
int no_of_vowels(char array[]){
    int c=0;
    for(int i=0;array[i] !='\0';i++){
        if(array[i] == 'a' || array[i] == 'e' || array[i] == 'i' || 
            array[i] == 'o' || array[i] == 'u' ||
            array[i] == 'A' || array[i] == 'E' || array[i] == 'I' || 
            array[i] == 'O' || array[i] == 'U'){
                        c++;
        }
    }
    return c;
}
int main(){
    printf("Enter your string : ");
    char pass[100];
    fgets(pass,sizeof(pass),stdin);
    printf("No of vowels in the string is : %d",no_of_vowels(pass));
    return 0;
}
