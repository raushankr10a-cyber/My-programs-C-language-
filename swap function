#include <stdio.h>
void swap(int *a,int* b){
    int q=*a;
    *a=*b;
    *b=q;
    return ;
}
int main() {
  printf("Enter the first number : y \n");
  int y;
  scanf("%d",&y);
  printf("Enter the second number : x\n");
  int x;
  scanf("%d",&x);
  printf("y : x =%d:%d\n",y,x);
  swap(&y,&x);
  printf("y : x =%d:%d\n",y,x);
    return 0;
}
